type User {
  id: ID! @id
  email: String! @unique
  password: String
  nickName: String!
  birth: String!
  googleId: String @unique
}

type Pomo {
  id: ID! @id
  createdBy: User!
  todo: [Todo]!
  date: String! @unique
}

type Todo {
  id: ID! @id
  createdBy: User!
  content: String!
  tag: [Tag]!
  status: String!
  start: String
  finish: String
}

type Tag {
  id: ID! @id
  content: String!
}